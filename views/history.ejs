<style>
  .timeline-section {
    position: relative;
    padding: 40px 0;
  }
  .timeline-section::before {
    content: '';
    position: absolute;
    left: 50%;
    transform: translateX(-50%);
    width: 4px;
    height: 100%;
    background: linear-gradient(180deg, var(--primary-color) 0%, var(--accent-color) 100%);
    top: 0;
  }
  .timeline-item {
    position: relative;
    margin-bottom: 60px;
  }
  .timeline-item:last-child {
    margin-bottom: 0;
  }
  .timeline-dot {
    position: absolute;
    left: 50%;
    transform: translateX(-50%);
    width: 24px;
    height: 24px;
    background: var(--primary-color);
    border: 4px solid var(--accent-color);
    border-radius: 50%;
    z-index: 2;
    top: 20px;
  }
  .timeline-content {
    width: 45%;
    padding: 30px;
    background: var(--white);
    border-radius: 16px;
    box-shadow: 0 10px 40px rgba(0,0,0,0.08);
    border: 1px solid rgba(74, 14, 25, 0.1);
    transition: all 0.3s ease;
  }
  .timeline-content:hover {
    transform: translateY(-5px);
    box-shadow: 0 15px 50px rgba(74, 14, 25, 0.15);
  }
  .timeline-item:nth-child(odd) .timeline-content {
    margin-left: auto;
    margin-right: 8%;
  }
  .timeline-item:nth-child(even) .timeline-content {
    margin-left: 8%;
  }
  .timeline-year {
    display: inline-block;
    padding: 8px 20px;
    background: var(--primary-color);
    color: var(--white);
    border-radius: 30px;
    font-weight: 600;
    font-size: 14px;
    margin-bottom: 16px;
  }
  .timeline-content h4 {
    color: var(--primary-color);
    margin-bottom: 12px;
    font-size: 22px;
  }
  .timeline-content p {
    color: var(--text-light);
    line-height: 1.7;
    margin-bottom: 0;
  }
  @media (max-width: 991px) {
    .timeline-section::before {
      left: 20px;
    }
    .timeline-dot {
      left: 20px;
    }
    .timeline-content {
      width: calc(100% - 60px);
      margin-left: 60px !important;
      margin-right: 0 !important;
    }
  }
  .milestone-box {
    background: var(--white);
    padding: 30px;
    border-radius: 16px;
    box-shadow: 0 10px 40px rgba(0,0,0,0.08);
    text-align: center;
    transition: all 0.3s ease;
    border: 1px solid rgba(74, 14, 25, 0.1);
    height: 100%;
  }
  .milestone-box:hover {
    transform: translateY(-5px);
    box-shadow: 0 15px 50px rgba(74, 14, 25, 0.15);
  }
  .milestone-box .icon {
    width: 80px;
    height: 80px;
    background: rgba(74, 14, 25, 0.1);
    border-radius: 50%;
    display: flex;
    align-items: center;
    justify-content: center;
    margin: 0 auto 20px;
  }
  .milestone-box .icon i {
    font-size: 32px;
    color: var(--primary-color);
  }
  .milestone-box h3 {
    color: var(--primary-color);
    font-size: 36px;
    font-weight: 700;
    margin-bottom: 8px;
  }
  .milestone-box p {
    color: var(--text-light);
    margin-bottom: 0;
  }
</style>

