<!--=====HEADER START=======-->
<header class="homepage5-body">
  <div id="vl-header-sticky" class="vl-header-area vl-transparent-header">
    <div class="container">
      <div class="row align-items-center">
        <div class="col-lg-2 col-md-6 col-6">
          <div class="vl-logo">
            <a href="/"><img src="/img/logo/logo1.png" alt=""></a>
          </div>
        </div>
        <div class="col-lg-7 d-none d-lg-block">
          <div class="vl-main-menu text-center">
            <nav class="vl-mobile-menu-active">
              <ul>
                <li><a href="/" class="<%= typeof currentPath !== 'undefined' && currentPath === '/' ? 'active' : '' %>">Home</a></li>
                <li><a href="/profile" class="<%= typeof currentPath !== 'undefined' && currentPath === '/profile' ? 'active' : '' %>">Profile</a></li>
                <li><a href="/produk" class="<%= typeof currentPath !== 'undefined' && currentPath === '/produk' ? 'active' : '' %>">Produk</a></li>
                <li><a href="/news" class="<%= typeof currentPath !== 'undefined' && currentPath === '/news' ? 'active' : '' %>">News & Event</a></li>
                <li><a href="/contact-us" class="<%= typeof currentPath !== 'undefined' && currentPath === '/contact-us' ? 'active' : '' %>">Contact Us</a></li>
                <li><a href="/csr" class="<%= typeof currentPath !== 'undefined' && currentPath === '/csr' ? 'active' : '' %>">CSR</a></li>
              </ul>
            </nav>
          </div>
        </div>
        <div class="col-lg-3 col-md-6 col-6">
          <div class="vl-hero-btn d-none d-lg-block text-end">
            <div class="head-btn">
              <ul>
                <% if (typeof settings !== 'undefined' && settings.site && settings.site.social) { %>
                  <% if (settings.site.social.instagram) { %><li><a href="<%= settings.site.social.instagram %>" target="_blank"><i class="fa-brands fa-instagram"></i></a></li><% } %>
                  <% if (settings.site.social.tiktok) { %><li><a href="<%= settings.site.social.tiktok %>" target="_blank"><i class="fa-brands fa-tiktok"></i></a></li><% } %>
                  <% if (settings.site.social.youtube) { %><li><a href="<%= settings.site.social.youtube %>" target="_blank"><i class="fa-brands fa-youtube"></i></a></li><% } %>
                  <% if (settings.site.social.tokopedia) { %><li><a href="<%= settings.site.social.tokopedia %>" target="_blank"><img src="/img/icons/tokopedia.svg" alt="Tokopedia" style="width: 16px; height: 16px;"></a></li><% } %>
                  <% if (settings.site.social.shopee) { %><li><a href="<%= settings.site.social.shopee %>" target="_blank"><img src="/img/icons/shopee.svg" alt="Shopee" style="width: 16px; height: 16px;"></a></li><% } %>
                <% } %>
              </ul>
            </div>
          </div>
          <div class="vl-header-action-item d-block d-lg-none">
            <button type="button" class="vl-offcanvas-toggle">
              <i class="fa-solid fa-bars-staggered"></i>
            </button>
          </div>
        </div>
      </div>
    </div>
  </div>
</header>
<!--=====HEADER END =======-->

<!--===== MOBILE HEADER STARTS =======-->
<div class="homepage5-body">
  <div class="vl-offcanvas">
    <div class="vl-offcanvas-wrapper">
      <div class="vl-offcanvas-header d-flex justify-content-between align-items-center mb-90">
        <div class="vl-offcanvas-logo">
          <a href="/"><img src="/img/logo/logo1.png" alt=""></a>
        </div>
        <div class="vl-offcanvas-close">
          <button class="vl-offcanvas-close-toggle"><i class="fa-solid fa-xmark"></i></button>
        </div>
      </div>

      <div class="vl-offcanvas-menu d-lg-none mb-40">
        <nav></nav>
      </div>

      <div class="space20"></div>
      <div class="vl-offcanvas-info">
        <h3 class="vl-offcanvas-sm-title">Contact Us</h3>
        <div class="space20"></div>
        <span><a href="tel:+6285189972223"> <i class="fa-regular fa-phone"></i> +62 851-8997-2223</a></span>
        <span><a href="mailto:info@produkpemuda.com"><i class="fa-regular fa-envelope"></i> info@produkpemuda.com</a></span>
        <span><a href="https://maps.google.com/?q=RRG+6+No.26,+Jaka+Setia,+Bekasi+Selatan,+Bekasi+17148" target="_blank"><i class="fa-regular fa-location-dot"></i> RRG 6 No.26, Jaka Setia, Bekasi Selatan, Bekasi 17148</a></span>
      </div>
      <div class="space20"></div>
      <div class="vl-offcanvas-social">
        <h3 class="vl-offcanvas-sm-title">Follow Us</h3>
        <div class="space20"></div>
        <% if (typeof settings !== 'undefined' && settings.site && settings.site.social) { %>
          <% if (settings.site.social.instagram) { %><a href="<%= settings.site.social.instagram %>" target="_blank"><i class="fab fa-instagram"></i></a><% } %>
          <% if (settings.site.social.tiktok) { %><a href="<%= settings.site.social.tiktok %>" target="_blank"><i class="fab fa-tiktok"></i></a><% } %>
          <% if (settings.site.social.youtube) { %><a href="<%= settings.site.social.youtube %>" target="_blank"><i class="fab fa-youtube"></i></a><% } %>
          <% if (settings.site.social.tokopedia) { %><a href="<%= settings.site.social.tokopedia %>" target="_blank"><img src="/img/icons/tokopedia.svg" alt="Tokopedia" style="width: 18px; height: 18px;"></a><% } %>
          <% if (settings.site.social.shopee) { %><a href="<%= settings.site.social.shopee %>" target="_blank"><img src="/img/icons/shopee.svg" alt="Shopee" style="width: 18px; height: 18px;"></a><% } %>
        <% } %>
      </div>

    </div>
  </div>
  <div class="vl-offcanvas-overlay"></div>
</div>
<!--===== MOBILE HEADER STARTS =======-->
